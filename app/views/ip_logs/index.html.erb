<p id="notice"><%= notice %></p>

<h1>Ip Logs</h1>

<table>
  <thead>
    <tr>
      <th>Ip address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ip_logs.each do |ip_log| %>
      <tr>
        <td><%= ip_log.ip_address %></td>
        <td><%= link_to 'Show', ip_log %></td>
        <td><%= link_to 'Edit', edit_ip_log_path(ip_log) %></td>
        <td><%= link_to 'Destroy', ip_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ip Log', new_ip_log_path %>
